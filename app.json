{
  "expo": {
    "name": "AnimFiFry",
    "slug": "rnAnimFiFry",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconv.png",
    "splash": {
      "image": "./assets/splashv.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.AnimFiFry"
    },
    "android": {
      "config": { 
        "googleMaps": { "apiKey": "Api-key-maps" } },
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconv.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.rnAnimFiFry.myapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
